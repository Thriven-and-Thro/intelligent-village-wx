<!--components/card/card.wxml-->
<view class="card">
  <view>
    <view class="card-header over">{{header}}</view>
    <view class="card-content over">{{content}}</view>
  </view>
  <view class="card-footer">
    <view class="footer-userid">{{userId}}</view>
    <view class="footer-count">{{count}}</view>
    <view class="footer-date">{{date}}</view>
  </view>
</view>