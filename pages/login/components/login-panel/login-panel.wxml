<!-- login-panel.wxml -->
<view class="login-panel">
  <form catchsubmit="formSubmit">
    <input type="text" name="name" value="{{formData.name}}" bindinput="modelValue" data-key="name"
      placeholder="请输入用户名" />
    <input type="password" name="password" value="{{formData.password}}" bindinput="modelValue" data-key="password"
      placeholder="请输入密码" />
    <checkbox checked="{{checked}}" bindtap="changeCheckBox"/>记住密码
    <button formType="submit">登录</button>
  </form>
</view>