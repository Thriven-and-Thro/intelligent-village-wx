.register-panel {
  font-size: 30rpx;
  padding: 5px;
}
.register-panel input {
  padding: 10rpx;
  margin-bottom: 36rpx;
  border-bottom: 1px solid #333333;
  font-size: 35rpx;
}
.register-panel button {
  width: 100%;
  text-align: center;
  line-height: 45rpx;
  margin: 60rpx 0 10rpx 0;
  background-image: linear-gradient(-20deg, #b721ff 0%, #21d4fd 100%);
  color: #fff;
}
