<!--pages/login/components/register-panel/register-panel.wxml-->
<view class="register-panel">
  <form catchsubmit="formSubmit">
    <input type="text" name="name" value="{{formData.name}}" bindinput="modelValue" data-key="name"
      placeholder="请输入您的用户名" />
    <input type="password" name="password" value="{{formData.password}}" bindinput="modelValue" data-key="password"
      placeholder="请输入您的密码" />
    <input type="password" name="confirmPassword" value="{{formData.confirmPassword}}" bindinput="modelConfirmPassword"
      data-key="confirmPassword" placeholder="请再次输入密码" />
    <button formType="submit">注册</button>
  </form>
</view>